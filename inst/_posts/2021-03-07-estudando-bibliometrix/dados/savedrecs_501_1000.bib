@article{ ISI:000319400400007,
Author = {Grafton, R. Quentin and Pittock, Jamie and Davis, Richard and Williams,
   John and Fu, Guobin and Warburton, Michele and Udall, Bradley and
   McKenzie, Ronnie and Yu, Xiubo and Che, Nhu and Connell, Daniel and
   Jiang, Qiang and Kompas, Tom and Lynch, Amanda and Norris, Richard and
   Possingham, Hugh P. and Quiggin, John},
Title = {{Global insights into water resources, climate change and governance}},
Journal = {{NATURE CLIMATE CHANGE}},
Year = {{2013}},
Volume = {{3}},
Number = {{4}},
Pages = {{315-321}},
Month = {{APR}},
Abstract = {{The threats of climate change and the trade-offs between extractions and
   flows are examined for the Colorado, the Murray, the Orange and the
   Yellow Rivers. In all four basins, and over a long period of time,
   outflows have greatly reduced as a direct result of increased water
   extractions. Although climate change will aggravate hydrological impacts
   on river systems, currently high levels of water extractions remain the
   principal contributor to reduced system flows. Changes in governance,
   including sharing the variability between the environment and consumers,
   are urgently required if the health of these rivers is to be maintained.}},
