@article{ ISI:000387849800003,
Author = {Yu, Haiyan},
Title = {{Can water users' associations improve water governance in China? A tale
   of two villages in the Shiyang River basin}},
Journal = {{WATER INTERNATIONAL}},
Year = {{2016}},
Volume = {{41}},
Number = {{7}},
Pages = {{966-981}},
Note = {{15th World Water Congress of the
   International-Water-Resources-Association (IWRA), Edinburgh, SCOTLAND,
   MAY 25-29, 2015}},
Organization = {{Int Water Resources Assoc}},
Abstract = {{A comparative study of two water users' associations in the Shiyang
   River basin, north-west China, shows significantly different
   understanding and experiences among water stakeholders. Factors other
   than formal organization appear to be more important in participatory
   irrigation management. For water users' associations to play a more
   significant role in water governance, they have to be context-specific,
   with better understanding of local perspectives, make strategic use of
   social capital, and cope with shared and/or conflicting interests among
   stakeholders.}},
