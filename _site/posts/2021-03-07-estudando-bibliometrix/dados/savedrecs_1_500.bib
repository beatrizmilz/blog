@article{ ISI:000426775500028,
Author = {Vannevel, Rudy},
Title = {{Using DPSIR and Balances to Support Water Governance}},
Journal = {{WATER}},
Year = {{2018}},
Volume = {{10}},
Number = {{2}},
Month = {{FEB}},
Abstract = {{The DPSIR framework has proven to be an effective communication tool,
   but has become subject to criticism upon increasing practical
   application. It is assumed that this is the consequence of the
   combination of terminological unclarity, absence of a methodological
   description for analyzing disturbances, and a confined conceptual basis
   when dealing with complex environmental issues. DPSIR has been embedded
   within the broader conceptual Pentatope Model (PTM), by making use of an
   `interface' called GASI. GASI, which means Governance by Actor-Subject
   Impact Assessment, deals explicitly with governance as a framework and
   impact analysis as a process. The result is an extended version of
   DPSIR. Illustrated by means of a water balance, the importance of a
   combined PTM-GASI-DPSIR tool for impact assessments to support
   evidence-based governance is shown.}},
