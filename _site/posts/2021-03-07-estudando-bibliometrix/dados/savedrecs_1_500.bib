@article{ ISI:000493446300003,
Author = {Tatar, M. and Papzan, A. and Ahmadvand, M.},
Title = {{Explaining the Good Governance of Agricultural Surface Water Resources
   in the Gawshan Watershed Basin, Kermanshah, Iran}},
Journal = {{JOURNAL OF AGRICULTURAL SCIENCE AND TECHNOLOGY}},
