@article{ ISI:000499771800007,
Author = {Pirsoul, Nicolas and Armoudian, Maria},
Title = {{Deliberative Democracy and Water Management in New Zealand: a Critical
   Approach to Collaborative Governance and Co-Management Initiatives}},
Journal = {{WATER RESOURCES MANAGEMENT}},
