@article{ ISI:000577108900001,
Author = {Latrubesse, Edgardo M. and Arima, Eugenio and Ferreira, Manuel E. and
   Nogueira, Sergio H. and Wittmann, Florian and Dias, Murilo S. and
   Dagosta, Fernando C. P. and Bayer, Maximiliano},
Title = {{Fostering water resource governance and conservation in the Brazilian
   Cerrado biome}},
Journal = {{CONSERVATION SCIENCE AND PRACTICE}},
Year = {{2019}},
Volume = {{1}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{The Brazilian Cerrado, one of the most threatened biomes of our planet,
   illustrates the challenges and opportunities of reconciling economic
   development with conservation of land and water ecosystems. Here, we
   assess the state of the art of and present new information on the
   impacts of agricultural expansion, dams, and water use, and make
   recommendations for basin management, conservation, and restoration of
   water-related Cerrado ecosystems and rivers. The conservation of the
   Cerrado requires not only the preservation of remnants of its vegetation
   but also the ability to keep the hydro-geomorphological and ecological
   functionality of its rivers, particularly the Araguaia River, the last
   large well preserved and unregulated system. If business as usual
   continues, the Cerrado riverine ecosystems may never rebound.}},
