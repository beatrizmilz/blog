@article{ ISI:000289840400001,
Author = {Bakker, Karen and Cook, Christina},
Title = {{Water Governance in Canada: Innovation and Fragmentation}},
Journal = {{INTERNATIONAL JOURNAL OF WATER RESOURCES DEVELOPMENT}},
Year = {{2011}},
Volume = {{27}},
Number = {{2}},
Pages = {{275-289}},
Abstract = {{This paper explores Canada's approach to water governance. It argues
   that fragmented governance has had negative impacts on Canada's ability
   to manage water resources adequately (particularly in the context of
   urbanization, agriculture and resource extraction), and to deal with new
   issues (such as climate change). Further, it argues that Canada's highly
   decentralized approach to water governance creates challenges of
   integration, coordination and data availability. The paper explores
   possible future strategies for innovations in water governance that may
   have the potential to improve water management outcomes.}},
