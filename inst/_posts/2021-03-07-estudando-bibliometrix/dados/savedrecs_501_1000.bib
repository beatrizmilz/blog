@article{ ISI:000327686500033,
Author = {Rouillard, J. J. and Heal, K. V. and Ball, T. and Reeves, A. D.},
Title = {{Policy integration for adaptive water governance: Learning from
   Scotland's experience}},
Journal = {{ENVIRONMENTAL SCIENCE \& POLICY}},
Year = {{2013}},
Volume = {{33}},
