@article{ ISI:000342827300005,
Author = {Levin-Keitel, Meike},
Title = {{Managing urban riverscapes: towards a cultural perspective of land and
   water governance}},
Journal = {{WATER INTERNATIONAL}},
Year = {{2014}},
Volume = {{39}},
Number = {{6, SI}},
Pages = {{842-857}},
Month = {{SEP 19}},
Abstract = {{Urban riverscapes are facing diverse demands concerning riparian uses,
   ecological, economic and social functionalities, and aesthetic
   questions. One of the main challenges today is the implementation of an
   integrative perspective on riverscapes to overcome the horizontal
   frontiers of traditional water management (water governance) and urban
   planning (land governance). Led by the theoretical framework of planning
   culture, the article shows the different rationalities and governance
   approaches from a cultural perspective. Finally, two quite different
   local planning cultures are outlined to illustrate the cultural variety
   with which the challenges of sustainable urban riverscapes are managed.}},
