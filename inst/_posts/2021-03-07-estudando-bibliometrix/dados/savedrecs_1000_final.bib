@article{ ISI:000249791900003,
Author = {Galaz, V.},
Title = {{Water governance, resilience and global environmental change - a
   reassessment of integrated water CO resources management (IWRM)}},
Journal = {{WATER SCIENCE AND TECHNOLOGY}},
Year = {{2007}},
