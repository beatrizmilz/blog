@article{ ISI:000469853400011,
Author = {Liu, Dengwei and Richards, Keith},
Title = {{The He-Zhang(River chief/keeper) system: an innovation in China's water
   governance and management}},
Journal = {{INTERNATIONAL JOURNAL OF RIVER BASIN MANAGEMENT}},
Year = {{2019}},
Volume = {{17}},
Number = {{2}},
Pages = {{263-270}},
Abstract = {{Governing and managing water sustainably is a critical issue globally.
   Given its rapid economic development and the effects of global climate
   change, China faces serious water challenges including water shortages
   and pollution. The Chinese government has long been considering
   governance reforms which will improve water and river management.
   Conscious of the negative effects of fragmented institutions and the
   lack of incentives to support environmentally-sustainable management,
   China has recently established and implemented the `He-Zhang' system - a
   network of River Chiefs and River Keepers across the country and at all
   administrative levels. This paper outlines the origins, purpose and
   structure of the system; summarizes progress with its implementation;
   and identifies some of its future challenges.}},
