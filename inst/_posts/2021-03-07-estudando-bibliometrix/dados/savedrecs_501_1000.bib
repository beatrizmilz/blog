@article{ ISI:000324172300005,
Author = {Schneider, Flurina and Homewood, Christine},
Title = {{Exploring Water Governance Arrangements in the Swiss Alps From the
   Perspective of Adaptive Capacity}},
Journal = {{MOUNTAIN RESEARCH AND DEVELOPMENT}},
Year = {{2013}},
Volume = {{33}},
Number = {{3}},
Pages = {{225-233}},
Month = {{AUG}},
Abstract = {{In times of increasing uncertainty because of climate and socioeconomic
   changes, the ability to deal with uncertainty and surprise is an
   essential requirement for the sustainability of alpine water governance.
   This article aims to contribute to the understanding of the adaptive
   capacity of water governance arrangements in the Swiss Alps and to
   propose options for reforms. To this purpose, we evaluated the current
   arrangements and the ways the actors have dealt with water shortages in
   the past, based on qualitative interviews and a document review. The
   research revealed that the adaptive capacity of the investigated
   arrangements is rather high with regard to reactive ways of responding
   to water shortage problems. However, there is limited capacity to
   proactively anticipate possible changes and to find prospective
   solutions on a regional scale. We conclude that with increased
   environmental and social pressures, forms of proactive water resource
   governance should be introduced, taking into account the welfare of
   people in both upstream and downstream areas.}},
Publisher = {{MOUNTAIN RESEARCH \& DEVELOPMENT}},
Address = {{BUSINESS OFFICE, 810 E 10TH ST, PO BOX 1897, LAWRENCE, KANSAS 66044-8897
   USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Schneider, F (Corresponding Author), Univ Bern, Ctr Dev \& Environm, Inst Geog, Hallerstr 10, CH-3012 Bern, Switzerland.
