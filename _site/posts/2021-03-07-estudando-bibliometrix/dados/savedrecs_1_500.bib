@article{ ISI:000475438500009,
Author = {Hassenforder, Emeline and Barone, Sylvain},
Title = {{Institutional arrangements for water governance}},
Journal = {{INTERNATIONAL JOURNAL OF WATER RESOURCES DEVELOPMENT}},
Year = {{2019}},
Volume = {{35}},
Number = {{5}},
Pages = {{778-802}},
Month = {{SEP 3}},
Abstract = {{Institutional arrangements shape water-related decision making and water
   policies, and drive behaviours related to water sharing and use. It is
   therefore crucial that water researchers, policy makers and managers
   understand institutions. This article reviews and critically assesses
   the current knowledge of water-related institutional arrangements. It
   explains how researchers explore institutional structures, emergence and
   change. It details how institutions are considered as a response to
   social-environmental issues through design, fit and bricolage. The
   article concludes with the most promising topics for the future
   scientific agenda.}},
