@article{ ISI:000295701100001,
Author = {Jaglin, Sylvy and Repussard, Clement and Belbeoc'h, Anne},
Title = {{Decentralisation and governance of drinking water services in small West
   African towns and villages (Benin, Mali, Senegal): the arduous process
   of building local governments}},
Journal = {{CANADIAN JOURNAL OF DEVELOPMENT STUDIES-REVUE CANADIENNE D ETUDES DU
   DEVELOPPEMENT}},
Year = {{2011}},
Volume = {{32}},
Number = {{2}},
Pages = {{119-138}},
Month = {{JUN}},
Abstract = {{This article analyses the development of decentralised water services in
   small West African towns and villages by focusing on the nature and
   dynamism of local project management arrangements. The paper will show
   how the capacity and legitimacy of local government has been undermined
   by competition with other forms of governance that focus on users
   committees or small private operators. It suggests that the pluralism in
   management approaches resulting from this accumulation of project-based
   models is not conducive to sustainable development and regulation of
   water services in small towns. Longer-term learning processes and
   alliances are required to craft workable co-production arrangements
