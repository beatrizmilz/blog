@article{ ISI:000416693500009,
Author = {Zwarteveen, Margreet and Kemerink-Seyoum, Jeltsje S. and Kooy, Michelle
   and Evers, Jaap and Guerrero, Tatiana Acevedo and Batubara, Bosman and
   Biza, Adriano and Boakye-Ansah, Akosua and Faber, Suzanne and Flamini,
   Andres Cabrera and Cuadrado-Quesada, Gabriela and Fantini, Emanuele and
   Gupta, Joyeeta and Hasan, Shahnoor and Ter Horst, Rozemarijn and Jamali,
   Hameed and Jaspers, Frank and Obani, Pedi and Schwartz, Klaas and
   Shubber, Zaki and Smit, Hermen and Torio, Phil and Tutusaus, Mireia and
   Wesselink, Anna},
Title = {{Engaging with the politics of water governance}},
Journal = {{WILEY INTERDISCIPLINARY REVIEWS-WATER}},
Year = {{2017}},
Volume = {{4}},
Number = {{6}},
Month = {{NOV-DEC}},
Abstract = {{The goal of the study is to strengthen the analytical purchase of the
   term water governance and improve the utility of the concept for
   describing and analyzing actual water distribution processes. We argue
   this is necessary as most writing on water governance is more concerned
   with promoting particular politically inspired agendas of what water
   governance should be than with understanding what it actually is. We
   believe that water governance at heart is about political choices as to
   where water should flow; about the norms, rules and laws on which such
   choices should be based; about who is best able or qualified to decide
   about this; and about the kind of societal future such choices support.
   We identify distributions-of water, voice and authority, and
   expertise-as the empirical anchor and entry-point of our
   conceptualization of water governance. This usefully allows
   foregrounding questions of equity in water governance discussions and
   provides the empirical foundation for a meaningful engagement with the
   politics of water governance. (C) 2017 The Authors. WIREs Water
   published by Wiley Periodicals, Inc.}},
