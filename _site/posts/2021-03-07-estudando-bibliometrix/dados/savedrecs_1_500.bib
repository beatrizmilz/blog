@article{ ISI:000488979500011,
Author = {Fustec, Klervi},
Title = {{A Sociopolitical Analysis of Drinking Water Governance in French
   Polynesia: The Case of the Tuamotu Archipelago}},
Journal = {{WATER ALTERNATIVES-AN INTERDISCIPLINARY JOURNAL ON WATER POLITICS AND
   DEVELOPMENT}},
Year = {{2019}},
Volume = {{12}},
Number = {{3}},
Pages = {{975-992}},
Month = {{OCT}},
Abstract = {{The assertion that only a small percentage of the French Polynesian
   population has access to drinking water is found in press reports and in
   reports by the French Senate and the French Polynesian Centre for
   Hygiene and Public Health, reports that were prepared in the context of
   implementing a new water law. In reality, however, inhabitants do have
   access to drinking water. How can we explain this discrepancy? This
   article analyses the sociopolitical dimensions of multilevel formal
   water governance in Tuamotu, one of the five French Polynesian
   archipelagos. Tuannotu's inhabitants use household rainwater harvesting
   cisterns for their drinking water provision. The analysis demonstrates
