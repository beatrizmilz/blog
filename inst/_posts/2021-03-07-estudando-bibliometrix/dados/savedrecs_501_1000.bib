@article{ ISI:000368711700009,
Author = {Hoogesteger, Jaime},
Title = {{NGOs and the Democratization of Ecuadorian Water Governance: Insights
   from the Multi-Stakeholder Platform el Foro de los Recursos Hidricos}},
Journal = {{VOLUNTAS}},
Year = {{2016}},
Volume = {{27}},
Number = {{1}},
Pages = {{166-186}},
Month = {{FEB}},
Abstract = {{NGOs have taken up an increasing number of roles and responsibilities in
   Latin American societies. Based on a study of the multi-stakeholder
   platform, the Water Resources Forum in Ecuador, this paper shows how
   through the creation of a broad network of NGOs, academics, grassroots
   water users organizations and governmental actors; this platform has
   been able to contribute to the democratization of water governance. This
   paper analyses the international and national socio-political context in
   which this platform developed and traces the history and strategies that
   marked its development. Based on this, it argues that NGOs can play an
   important role in the development of more democratic and inclusive
   public policy making in water governance, but that the capacity of NGOs
   to bring about change greatly depends on the socio-political context and
   on the networks they are able to forge with grassroots organizations,
   state agencies, funders and other third sector actors.}},
