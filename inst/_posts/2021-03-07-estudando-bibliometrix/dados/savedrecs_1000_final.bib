@article{ ISI:000088381300004,
Author = {Asmal, K},
Title = {{Water as a metaphor for governance: issues in water resources management
   in Africa}},
Journal = {{NATURE \& RESOURCES}},
Year = {{1998}},
Volume = {{34}},
Number = {{1}},
Pages = {{19-25}},
Month = {{JAN-MAR}},
Abstract = {{As we approach a new century and a new millennium, much attention is
   being given to water and our management of it, reflected in 1997 in the
   diffusion of a comprehensive assessment of the freshwater resources of
   the world and in the convening in Morocco of the First World Water
   Forum. Events such as these have served to bring new perspectives to
   bear on the management of water resources. An example is in South
   Africa, where water and its management is being viewed at the highest
   levels of government as a metaphor for development and people's
   relations with one another. Here, the old heroic concept of water
   resource development, which challenged engineers to make water available
   to those who needed it, has been replaced by a more modest vision - that
   of managing the quantity, quality and reliability of the nation's water
   resources to achieve optimum long-term, environmentally sustainable,
   social and economic development for society from their use.}},
