@article{ ISI:000452262700006,
Author = {Ben Nasr, Jamel and Bachta, Mohamed Salah},
Title = {{Conflicts and water governance challenge in irrigated areas of semi-arid
   regions}},
Journal = {{ARABIAN JOURNAL OF GEOSCIENCES}},
