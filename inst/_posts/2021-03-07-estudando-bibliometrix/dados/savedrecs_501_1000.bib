@article{ ISI:000305255400008,
Author = {Sojamo, Suvi and Keulertz, Martin and Warner, Jeroen and Allan, John
   Anthony},
Title = {{Virtual water hegemony: the role of agribusiness in global water
   governance}},
Journal = {{WATER INTERNATIONAL}},
Year = {{2012}},
Volume = {{37}},
Number = {{2, SI}},
Pages = {{169-182}},
Abstract = {{The recent global food crises have highlighted how the agro-food system
   tends to be subject to powerful agribusiness players, with thus far
   unidentified consequences for global water security. By connecting
   hydro-hegemony and virtual water concepts, this study illustrates the
   Western dominance over the virtual water embedded in international
   agro-food commodity trade flows. Accordingly, foreign direct investment
   in land by emerging Asian and Arab economies and their increased
   competition over the sources of global food supply chains appear as
   strategies to challenge the Western agribusiness ``virtual water
   hegemony{''}.}},
