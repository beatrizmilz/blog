@article{ ISI:000259351100004,
Author = {Hossain, Farhad and Helao, Tuhafeni},
Title = {{Local governance and water resource management: Experiences from
   northern Namibia}},
Journal = {{PUBLIC ADMINISTRATION AND DEVELOPMENT}},
