Title = {{Irrigation and water security:the role of economic instruments and
   governance}},
Booktitle = {{SUSTAINABLE IRRIGATION MANAGEMENT, TECHNOLOGIES AND POLICIES II}},
Series = {{WIT Transactions on Ecology and the Environment}},
Year = {{2008}},
Volume = {{112}},
