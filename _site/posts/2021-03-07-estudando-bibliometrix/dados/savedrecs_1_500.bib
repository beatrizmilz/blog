@article{ ISI:000531884900012,
Author = {Weng, Lie En and Wang, Hui Yu and Jiang, Han Ting and GU, Ye Heng and
   HU, Shui Gen},
Title = {{CONSTRUCTION AND EMPIRICAL STUDY ON THE EFFECT MODEL OF WATER
   ENVIRONMENTAL GOVERNANCE IN CHINA}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2019}},
Volume = {{20}},
Number = {{B}},
Pages = {{S475-S485}},
Abstract = {{In order to evaluate the efficiency of China water environmental
   governance and its changing trend, this study uses the DEA method and
   BCC model to analyse 4 aspects data of 31 provincial-level
   administrative units in mainland of China from 2008 to 2017. As the
   decision-making unit of analysis and evaluation, DEAP is used as an
   analysis tool to analyse the input-output efficiency of water resources
   environmental governance. The results show that in recent years, most
   provinces in mainland of China have increased the input of human
   investment, material investment and financial investment for water
   resources environmental governance, while the input-output efficiency
   ratio is relatively low. Applying the results of the study to policy
   assessments can provide references for improving water environmental
   governance performance. The regulation methods of water environment need
   to be improved, and the technical level of environmental protection
   investment should be improved.}},
