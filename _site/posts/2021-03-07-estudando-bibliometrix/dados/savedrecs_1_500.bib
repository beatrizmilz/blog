@article{ ISI:000472390200001,
Author = {Wuijts, Susanne and Beekman, Jappe and van der Wal, Bas and Suykens,
   Cathy and Driessen, Peter P. J. and Van Rijswick, Helena F. M. W.},
Title = {{An ecological perspective on a river's rights: a recipe for more
   effective water quality governance?}},
Journal = {{WATER INTERNATIONAL}},
Year = {{2019}},
Volume = {{44}},
Number = {{6-7, SI}},
Pages = {{647-666}},
Month = {{OCT 3}},
Abstract = {{In several countries, the transfer of legal rights to rivers is being
   discussed as an approach for more effective water resources management.
   But what could this transfer mean in terms of a healthy river? We
   address this question by identifying the ecological requirements for
   naturally functioning rivers and then explore the demands which these
   requirements impose on society, the current policy responses to these
   requirements and whether the transfer of rights to the river could
   facilitate the preservation of healthy freshwater ecosystems.}},
