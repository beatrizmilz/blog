@article{ ISI:000277962500002,
Author = {Magel, Holger and Gross, Christiane},
Title = {{Land and Water Governance - a Topic for Germany?}},
Journal = {{WASSERWIRTSCHAFT}},
Year = {{2010}},
Volume = {{100}},
Number = {{5}},
Pages = {{10-15}},
Abstract = {{In the context of climate and demographic changes, conflicts in access
   to and use of the resources land and water are going to increase. Every
   day it becomes more evident that these upcoming challenges can only be
   resolved through a holistic common understanding and management of land
   and water. International demands already go beyond integrated management
   models. The paper addresses the need fora combined Land and Water
   Governance - a governance which makes reference to land, water, property
   and use questions in a simultaneous and holistic manner. Appropriate
   discussions in this regard are missing in Germany: are they unnecessary
   or do we need to catch up?}},
