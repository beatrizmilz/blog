@article{ ISI:000261934800005,
Author = {Norman, Emma S. and Bakker, Karen},
Title = {{Transgressing Scales: Water Governance Across the Canada-US Borderland}},
Journal = {{ANNALS OF THE ASSOCIATION OF AMERICAN GEOGRAPHERS}},
