@article{ ISI:000603070800033,
Author = {Sarkar, Uma Dey and Choudhary, Bikramaditya K.},
Title = {{Reconfiguring urban waterscape: water kiosks in Delhi as a new
   governance model}},
Journal = {{JOURNAL OF WATER SANITATION AND HYGIENE FOR DEVELOPMENT}},
