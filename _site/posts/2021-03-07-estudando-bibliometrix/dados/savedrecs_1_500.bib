@article{ ISI:000434334100014,
Author = {King-Okumu, Caroline and Jillo, Bashir and Kinyanjui, John and Jarso,
   Ibrahim},
Title = {{Devolving water governance in the Kenyan Arid Lands: from top-down
   drought and flood emergency response to locally driven water resource
   development planning}},
Journal = {{INTERNATIONAL JOURNAL OF WATER RESOURCES DEVELOPMENT}},
Year = {{2018}},
Volume = {{34}},
Number = {{4, SI}},
Pages = {{675-697}},
Abstract = {{The Kenyan Constitution calls for a devolved response to the stewardship
   of water and other natural resources. A case study based on planners'
   experiences illustrates the shift towards a governance approach that is
   inclusive, integrates available technologies to achieve resilience to
   both flood and drought, and works across scales from the settlement to
   the catchment. Devolution is a slow process, and the challenges are
   many. Recent observations show that increasing local agency in water
   resource development is helping alleviate drought and flood emergencies.
   Nevertheless, more concerted action is still needed from the centre.}},
