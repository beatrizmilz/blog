@article{ ISI:000380595700003,
Author = {Montgomery, Jenna and Xu, Wei and Bjornlund, Henning and Edwards, Jane},
Title = {{A table for five: Stakeholder perceptions of water governance in Alberta}},
Journal = {{AGRICULTURAL WATER MANAGEMENT}},
