@article{ ISI:000459735100135,
Author = {Grafton, R. Quentin and Garrick, Dustin and Manero, Ana and Thang Nam Do},
Title = {{The Water Governance Reform Framework: Overview and Applications to
   Australia, Mexico, Tanzania, USA and Vietnam}},
