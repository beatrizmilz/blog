@article{ ISI:000300817500008,
Author = {Huntjens, Patrick and Lebel, Louis and Pahl-Wostl, Claudia and Camkin,
   Jeff and Schulze, Roland and Kranz, Nicole},
Title = {{Institutional design propositions for the governance of adaptation to
   climate change in the water sector}},
